html {
    padding-bottom: 14.53125vw;
}

.searchBox {
    width: 100%;
    height: 10.78125vw;
    background: #333333;
    padding: 1.625vw 2.96875vw;
}
.rightMore {
    float: right;
    display: flex;
    justify-content: start;
    width: 13vw;
}
.lookMoreMeun {
    display: inline-block;
    width: 5.1875vw;
    height: 5.1875vw;
    background: url(/static/images/Page/anyMore.png) no-repeat center center;
    background-size: 100% 100%;
    margin-top: 1.225vw;
    margin-right: 1vw;
    z-index: 1000;
}
.searchBox>h1 {
    float: left;
    font-size: 4.5vw;
    font-weight: bold;
    height: 100%;
    margin-right: 21vw;
    line-height: 8vw;
    color:#fff;
}
.setting_bottom_box{
    position: fixed;
    width: 100vw;
    background: #fff;
    z-index:100;
}
.settingMenuBox{
    position: relative;
    top:0;
    left: 50%;
    transform: translateX(-50%);
    max-width: 1024px;
    width: 100vw;
    background: #fff;
    box-sizing: border-box;
    display:flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    padding-bottom:5vw;
}
.settingMenuBox>a{
    height: 100%;
    width: 25vw;
    display: inline-block;
    margin-top: 5vw;
}
.settingMenuBox>a>div{
    width:7.8vw;
    height:7.8vw;
    margin:0 auto;
}
.settingMenuBox>a>span{
    display: block;
    margin:2vw auto 0;
    text-align: center;
    color:#000;
}
.myshefbook_box{
    text-align: center;
    border-top: .3vw solid #999;
    margin: 0 auto;
    width: 80vw;
}
.toMySelfBook{
    display: inline-block;
    border-radius: 5vw;
    border: .3vw solid #000;
    width: 40.15625vw;
    height: 10vw;
    line-height: 10vw;
    margin-top: 4vw;
    background: #000;
    color: #fff;
}
.fl_imgmark{
    background: url(/static/images/Page/fl_.png) no-repeat center center;
    background-size: 100% 100%;
    border-radius: 50%;
}
.ph_imgmark{
    background: url(/static/images/Page/ph_.png) no-repeat center center;
    background-size: 100% 100%;
    border-radius: 50%;
}
.allbook_imgmark{
    background: url(/static/images/Page/allbook_.png) no-repeat center center;
    background-size: 100% 100%;
    border-radius: 50%;
}
.newbook_imgmark{
    background: url(/static/images/Page/newbook_.png) no-repeat center center;
    background-size: 100% 100%;
    border-radius: 50%;
}
.index_imgmark{
    background: url(/static/images/Page/index_.png) no-repeat center center;
    background-size: 100% 100%;
    border-radius: 50%;
}
.me_imgmark{
    background: url(/static/images/Page/me_.png) no-repeat center center;
    background-size: 100% 100%;
    border-radius: 50%;
}
.searchBtn {
    float: right;
    width: 4.6vw;
    height: 5.1875vw;
    background: url(/static/images/Page/searchBtn.png) no-repeat center center;
    background-size: 100% 92%;
    margin-top: 1.325vw;
    margin-right: 1.78125vw;
}

.searchBtn>a {
    display: block;
    width: 100%;
    height: 100%;
}

.channel {
    float: left;
    border-radius: 5.5vw;
    border: 1px solid #ddd;
    color: #ddd;
    margin-top: 1vw;
    height: 5.5vw;
    line-height: 5.5vw;
}

.channel>.cur {
    background: #fff;
    color: #333333;
}

.channel>span {
    width: 8.96874vw;
    height: 100%;
    border-radius: 5.5vw;
    float: left;
    text-align: center;
}

.indexBanner {
    width: 100%;
    height: 35.9375vw;
    overflow: hidden;
}

.indexBannerSlide>a {
    width: 100%;
    height: 35.9375vw;
}

.indexBannerSlide>a>img {
    width: 100%;
    height: 35.9375vw;
}

.continueReading {
    width: 100%;
    background: #fff;
}

.continueReading>a {
    display: block;
    height: 13vw;
}

.continueTitle {
    width: 21.71875vw;
    height: 100%;
    float: left;
    margin-left: 2.1875vw;
}

.continueTitle>.time_icon {
    width: 4.375vw;
    height: 4.375vw;
    float: left;
    margin-left: 1.71875vw;
    margin-top: 3.9375vw;
    background: url(/static/images/Page/time.png);
    background-size: 100% 100%;
}

.continueTitle>span {
    float: left;
    width: 15.625vw;
    height: 100%;
    line-height: 12.8124vw;
    font-size: 3.18125vw;
    color: #000;
    box-sizing: border-box;
    padding-left: 0.625vw;
}

.continueReading>a>i {
    float: left;
    width: 0.46875vw;
    height: 4.0625vw;
    background: #000;
    margin-top: 4.0625vw;
}

.continueCont {
    width: 62.65625vw;
    height: 100%;
    float: left;
    padding-left: 2.34375vw;
    font-family: '微软雅黑';
}

.continueName {
    width: 100%;
    height: 50%;
    display: block;
    color: #000;
    font-size: 3.6234375vw;
    line-height: 8.4062vw;

}

.continueChapter {
    width: 100%;
    height: 50%;
    display: block;
    color: #575757;
    font-size: 3.221875vw;
    line-height: 4.4062vw;
    padding-left: 2vw;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.copyright_info {
    border-top: 3px solid #eee;
    width: 100%;
    height: auto;
    padding: 3vw;
    background:#fff;
}

.copyright_info>p {
    margin-bottom: 2vw;
    text-align: center;
    font-size: 3.4vw;
    color: #777;
}
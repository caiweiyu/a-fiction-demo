}

.imgSrc{
    background: url(/static/images/Page/book/book01.png);
    background-size: 100% 100%;
    height: 31vw;
    width: 26.09375vw;
    box-shadow:2px 10px 10px 0 #e0e0e0;
    margin-bottom: -9vw;
    margin-left:4vw;
}
.imgSrc_t{
